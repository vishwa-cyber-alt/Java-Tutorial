class HelloWorld {
    public static void main(String[] args) {
        int age[]={20,40,50,60};
        
        for(int i=0;i<age.length;i++)
        {
            if(age[i]>43)
            
            {
                System.out.print(" eligible");
            }
            else{
                System.out.print(" not eligible");
            }
        }
    }
}
